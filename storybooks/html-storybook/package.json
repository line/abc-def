{
  "name": "@abc-def/html-storybook",
  "private": true,
  "version": "0.1.1",
  "type": "module",
  "scripts": {
    "dev:storybook": "storybook dev -p 6007 --no-open",
    "build:storybook": "storybook build -o dist",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "clean": "git clean -xdf .turbo node_modules storybook-static dist .cache",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "lit": "^3.3.1"
  },
  "devDependencies": {
    "@abc-def/eslint-config": "workspace:^",
    "@abc-def/prettier-config": "workspace:^",
    "@abc-def/tailwindcss": "workspace:^",
    "@abc-def/tsconfig": "workspace:^",
    "@chromatic-com/storybook": "catalog:storybook",
    "@storybook/addon-docs": "catalog:storybook",
    "@storybook/web-components": "catalog:storybook",
    "@storybook/web-components-vite": "catalog:storybook",
    "@types/react": "catalog:react",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "storybook": "catalog:storybook",
    "typescript": "catalog:",
    "vite": "^7.0.6"
  },
  "prettier": "@abc-def/prettier-config"
}
