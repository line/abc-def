{
  "name": "@line/abc-def-react",
  "version": "1.0.0",
  "description": "React components for the ABC Def design system",
  "license": "Apache-2.0",
  "homepage": "https://line.github.io/abc-def/",
  "repository": {
    "type": "git",
    "url": "https://github.com/line/abc-def.git",
